{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultProject": "frontend",
  "npmScope": "anonymous-systems",
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "{projectRoot}/**/*",
        "{projectRoot}/.env*"
      ],
      "outputs": [
        "{projectRoot}/.next/**",
        "!{projectRoot}/.next/cache/**"
      ],
      "cache": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": true
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ],
      "cache": true
    },
    "dev": {
      "cache": false
    }
  }
}