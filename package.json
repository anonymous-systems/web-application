{
  "name": "anonymous-systems",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel --ui=stream",
    "dev:ui": "turbo dev",
    "dev:emulators": "firebase emulators:exec \"pnpm dev\" --ui",
    "dev:seed": "firebase emulators:exec \"pnpm dev\" --import=seed --export-on-exit=seed --ui",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "firebase:emulators": "firebase emulators:start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "firebase emulators:exec \"concurrently --kill-others \\\"pnpm dev\\\" \\\"pnpm cypress:run\\\"\" --import=test-seed --export-on-exit=test-seed",
    "test:ui": "firebase emulators:exec \"concurrently \\\"pnpm dev\\\" \\\"pnpm cypress:open\\\"\" --import=test-seed --export-on-exit=test-seed --ui"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/firebase-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.3",
    "firebase": "^12.0.0",
    "prettier": "^3.5.1",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "engines": {
    "node": ">=20"
  },
  "type": "module"
}
